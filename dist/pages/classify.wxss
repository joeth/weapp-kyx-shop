.classify{flex:1;width:100%;overflow:hidden;position:relative;background-color:#e9e0cc}.classify-nav{width:200rpx;position:absolute;bottom:0;left:0;top:0;background-image:url('https://t1.picb.cc/uploads/2018/02/05/sqzLX.jpg')}.classify-nav .nav-item{height:100rpx;line-height:100rpx;overflow:hidden;padding:0 15rpx;display:-webkit-box;word-break:break-all;-webkit-line-clamp:1;-webkit-box-orient:vertical;border-top:2rpx solid #b9ae8b;text-align:center;font-size:32rpx}.classify-nav .nav-item:first-child{border-top:none}.classify-nav .nav-item.active{background-color:#e9e0cc}.classify-body{height:100%;overflow:auto;position:absolute;bottom:0;left:200rpx;top:0}