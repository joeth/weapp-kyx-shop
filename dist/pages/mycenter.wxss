.navbar{padding:10rpx 0;background-color:#e9e0cc}.navbar .navbar-item{padding:20rpx 0;position:relative}.navbar .navbar-item:before{content:'';height:1em;width:2rpx;margin-top:-0.5em;position:absolute;top:50%;left:0;background-color:#b9ae8b}.navbar .navbar-item:first-child:before{display:none}.navbar .navbar-badge{position:absolute;right:10rpx;top:0}.header{padding:60rpx;text-align:center;background-color:#452713}.header .user-info-avatar{width:170rpx;height:170rpx;overflow:hidden;border:5rpx solid #fff;border-radius:50%}.header .user-info-name{margin-top:20rpx;color:#d6c5ac}.menu{display:block;padding:60rpx;margin-top:20rpx;background-color:#e9e0cc}.menu .menu-button{padding:20rpx}.menu .icon,.menu .text{display:block;text-align:center}.menu .icon{font-size:64rpx;margin-bottom:20rpx}