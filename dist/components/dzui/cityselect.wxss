.dzui-cityselect{width:100%;background-color:#fff;position:relative;z-index:1000}.dzui-cityselect-cancel,.dzui-cityselect-confirm{display:block;border:none;outline:none;margin:0;padding:1em 1.5em;height:auto;line-height:inherit;border-radius:0;background-color:transparent;color:#8c8174;font-size:34rpx}.dzui-cityselect-cancel:after,.dzui-cityselect-confirm:after{display:none}.dzui-cityselect-cancel.button-hover,.dzui-cityselect-confirm.button-hover{background-color:#f6f1dd}.dzui-cityselect-confirm{color:#a91e11}.dzui-cityselect-tool{display:flex;align-items:center;justify-content:space-between;background-color:#e9e0cc}.dzui-cityselect-view{display:block;height:12em;padding:0 .5em;background-color:white}.dzui-cityselect-text{height:100%;line-height:1.5em;padding:0 .5em;display:flex;align-items:center;justify-content:center;text-align:center}